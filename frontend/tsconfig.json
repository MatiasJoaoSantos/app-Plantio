{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
    "@/*": ["./src/*"],
    "@/constants/*": ["./constants/*"],
    "@/hooks/*": ["./hooks/*"],
    "@/components/*": ["./src/components/*"],
    "@/components/ui/*": ["./src/components/ui/*"],
    "@/assets/*": ["./assets/*"]
  }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "frontend/.expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

